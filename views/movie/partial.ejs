<div class="col-sm-12 col-md-3">
	<div class="thumbnail movie-tile">
	  <img src="<%= data.poster_url %>" alt="<%= data.title %>">
	  <div class="caption">
	    <h3><%= data.title %></h3>
			<h4><%= data.release_date %></h4>
			<h4><%= __('Rate')%> : <%= data.vote_average %></h4>
	    <p><%= data.synopsis %></p>
	    <p>
				<a href="/movie/play/<%= data.id %>" class="btn btn-primary" role="button"><%= __('Play')%></a>
			</p>
	  </div>
	</div>
</div>
